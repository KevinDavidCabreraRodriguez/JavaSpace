<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="empezarPartidaHoverRoot" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="imageViewEP" fitHeight="800.0" fitWidth="1500.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="launchButton" alignment="BOTTOM_CENTER" contentDisplay="BOTTOM" mnemonicParsing="false" styleClass="launchButton" text="JUGAR" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </Button>
      <ImageView fx:id="loadingImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <StackPane.margin>
            <Insets top="200.0" />
         </StackPane.margin>
      </ImageView>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</StackPane>
